{
  "name": "wellcart/wellcart",
  "description": "WellCart Application",
  "license": "BSD-3-Clause",
  "homepage": "http://wellcart.org/",
  "type": "project",
  "authors": [
    {
      "name": "Dima Denisenko",
      "email": "dima@wellcart.org",
      "role": "Developer"
    },
    {
      "name": "WellCart Application Contributors",
      "homepage": "https://github.com/wellcart/wellcart/contributors"
    }
  ],
  "keywords": [
    "Zend Framework",
    "Open-source"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1.0",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-openssl": "*",
    "lib-pcre": ">=7.3",
    "ext-iconv": "*",
    "ext-mbstring": "*",
    "ext-xml": "*",
    "ext-libxml": "*",
    "ext-intl": "*",
    "jbroadway/urlify": "1.*",
    "patchwork/utf8": "1.*",
    "raulfraile/ladybug": "1.*",
    "nesbot/carbon": "1.*",
    "ext-PDO": "*",
    "ext-pdo_mysql": "*",
    "lib-libxml": "*",
    "ext-ctype": "*",
    "ext-hash": "*",
    "ext-spl": "*",
    "ext-gd": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-xsl": "*",
    "antasoft/web-components": "^0.1.0",
    "vlucas/phpdotenv": "^2.4",
    "zendframework/zend-router": "3.*",
    "zendframework/zend-servicemanager": "2.7.*",
    "zendframework/zend-config-aggregator": "1.*",
    "zendframework/zend-developer-tools": "1.1.*",
    "container-interop/container-interop": "^1.2.0",
    "gedmo/doctrine-extensions": "2.4.*",
    "doctrine/doctrine-module": "^1.2.0",
    "beberlei/DoctrineExtensions": "1.*",
    "bitweb/zf2-cron-module": "2.0.*",
    "bjyoungblood/bjy-profiler": "1.*",
    "rwoverdijk/assetmanager": "~1.7.0",
    "tck/zf2-imageresizer": "1.*",
    "acelaya/zf2-acmailer": "5.*",
    "neilime/zf2-twb-bundle": "3.*@stable",
    "zfc-datagrid/zfc-datagrid": "1.3.*",
    "soflomo/purifier": "1.0.*",
    "hounddog/doctrine-data-fixture-module": "0.*",
    "firephp/firephp-core": "0.4.*",
    "ccampbell/chromephp": "4.*",
    "oro/doctrine-extensions": "^1.0.0",
    "zf-commons/zfc-user": "~1.4.0",
    "zf-commons/zfc-user-doctrine-orm": "1.*",
    "zf-commons/zfc-rbac": "^2.6.0",
    "zf-commons/zfc-admin": "0.2.*",
    "robmorgan/phinx": "0.8.*",
    "fzaninotto/faker": "1.6.*",
    "simple-bus/asynchronous": "2.*",
    "symfony/options-resolver": "~2.6"
  },
  "require-dev": {
    "phpunit/phpunit-skeleton-generator": "*",
    "phpunit/phpunit": "^5.7.22",
    "edgedesign/phpqa": "^1.12.1",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "jakub-onderka/php-console-highlighter": "^0.3.2",
    "phpstan/phpstan": "^0.7",
    "satooshi/php-coveralls": "~1.0"
  },
  "suggest": {
    "ext-xdebug": "Extension which provides debugging and profiling capabilities, make possible code coverage support."
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "process-timeout": 4000,
    "preferred-install": "dist",
    "component-dir": "public/assets/lib",
    "component-baseurl": "/assets/lib"
  },
  "scripts": {
    "post-install-cmd": [
      "WellCart\\Setup\\Composer\\ScriptHandler::createProjectDirectories",
      "WellCart\\Setup\\Composer\\ScriptHandler::setPermissions"
    ],
    "post-update-cmd": [
      "WellCart\\Setup\\Composer\\ScriptHandler::createProjectDirectories",
      "WellCart\\Setup\\Composer\\ScriptHandler::setPermissions"
    ],
    "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
    "coveralls": "coveralls",
    "cs": "phpcs",
    "cs-fix": "phpcbf"
  },
  "extra": {
    "wellcart": {
      "type": "project",
      "context": {
        "global": {
          "name": "global",
          "extends": false,
          "require": []
        },
        "common": {
          "name": "common",
          "extends": false,
          "exclude": [
            {"name": "WellCart\\Setup", "priority": 70000}
          ],
          "require": [
            {"name": "WellCart\\Ui\\Layout", "priority": -90500},
            {"name": "WellCart\\Theme\\FrontendUi", "priority": -90000},
            {"name": "ZfcBase", "priority": -72948},
            {"name": "AcMailer", "priority": -72943},
            {"name": "BjyProfiler", "priority": -72941},
            {"name": "ZendDeveloperTools", "priority": -72940},
            {"name": "WellCart\\CommandBus", "priority": -72939},
            {"name": "WellCart\\SchemaMigration", "priority": -72938},
            {"name": "AssetManager", "priority": -72935},
            {"name": "TwbBundle", "priority": -72934},
            {"name": "ZfcDatagrid", "priority": -72932},
            {"name": "WellCart\\Form\\JsValidation", "priority": -72930},
            {"name": "TckImageResizer", "priority": -72929},
            {"name": "WellCart\\Ui\\Wizard", "priority": -72927},
            {"name": "Soflomo\\Purifier", "priority": -72925},
            {"name": "DoctrineModule", "priority": -72924},
            {"name": "DoctrineORMModule", "priority": -72922},
            {"name": "DoctrineDataFixtureModule", "priority": -72920},
            {"name": "BitWeb\\CronModule", "priority": -72917},
            {"name": "WellCart\\Base", "priority": -72900},
            {"name": "ZfcUser", "priority": -72852},
            {"name": "ZfcUserDoctrineORM", "priority": -72851},
            {"name": "ZfcRbac", "priority": -72850},
            {"name": "WellCart\\User", "priority": -72820},
            {"name": "ZfcAdmin", "priority": -72810},
            {"name": "WellCart\\Backend", "priority": -72800},
            {"name": "WellCart\\CMS", "priority": 20},
            {"name": "WellCart\\Money", "priority": 29},
            {"name": "WellCart\\Directory", "priority": 30},
            {"name": "WellCart\\Catalog", "priority": 40}
          ]
        },
        "console": {
          "name": "console",
          "extends": "common",
          "require": []
        },
        "frontend": {
          "name": "frontend",
          "extends": "common",
          "require": [
            {"name": "WellCart\\Theme\\BackendUi", "priority": -80400}
          ]
        },
        "backend": {
          "name": "backend",
          "extends": "frontend",
          "require": [
            {"name": "WellCart\\Theme\\BackendUi", "priority": -80400}
          ]
        },
        "setup": {
          "name": "setup",
          "extends": false,
          "require": [
            {"name": "WellCart\\Ui\\Layout", "priority": -90500},
            {"name": "WellCart\\Theme\\FrontendUi", "priority": -90000},
            {"name": "WellCart\\Theme\\SetupUi", "priority": -80200},
            {"name": "ZfcBase", "priority": -72931},
            {"name": "BjyProfiler", "priority": -72925},
            {"name": "AssetManager", "priority": -72920},
            {"name": "TwbBundle", "priority": -72919},
            {"name": "WellCart\\Form\\JsValidation", "priority": -72915},
            {"name": "WellCart\\CommandBus", "priority": -72913},
            {"name": "WellCart\\Ui\\Wizard", "priority": -72912},
            {"name": "WellCart\\Base", "priority": -72900},
            {"name": "ZendDeveloperTools", "priority": -72938},
            {"name": "WellCart\\Setup", "priority": 70000}
          ]
        }
      }
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev",
      "dev-develop": "0.2.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "WellCart\\Base\\": "component/base/src/",
      "WellCart\\Backend\\": "component/backend/src/",
      "WellCart\\Setup\\": "component/setup/src/",
      "WellCart\\User\\": "component/user/src/",
      "WellCart\\Catalog\\": "module/catalog/src/",
      "WellCart\\CMS\\": "module/cms/src/",
      "WellCart\\Directory\\": "module/directory/src/"
    },
    "files": [
      "component/base/src/functions.php",
      "component/library/utility/src/functions.php"
    ],
    "classmap": [
      "component/library/",
      "component/library/mapping-drivers/",
      "component/schema-migration/src/",
      "component/command-bus/src/",
      "component/library/utility/src/",
      "theme/frontend-ui/",
      "theme/backend-ui/",
      "theme/setup-ui/"
    ]
  }
}
