    "components/handlebars.js": "4.0.*",
    "tinymce/tinymce": "4.4.*",
    "blueimp/jquery-file-upload": "9.12.*",
    "zfcampus/zf-apigility": "1.*@dev",
    "zfcampus/zf-apigility-documentation": "~1.0-dev",
    "zfcampus/zf-apigility-documentation-swagger": "~1.0-dev",
    "zfcampus/zf-api-problem": "1.*@dev",
    "zfcampus/zf-apigility-doctrine": "^2.0.5",
    "zfcampus/zf-content-validation": "^1.3.2",
    "zfcampus/zf-doctrine-querybuilder": "1.*@dev",
    "api-skeletons/zf-oauth2-doctrine": "1.0.5",
    "api-skeletons/zf-oauth2-doctrine-console": "1.2.1",
    "zfcampus/zf-deploy": "1.*",
    "zf-commons/zfc-user": "~1.4.0",
    "zf-commons/zfc-user-doctrine-orm": "1.*",
