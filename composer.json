    "neeckeloo/wizard": "1.0.1",
    "stroker/form": "0.2.0",
    "radnan/rdn-csv": "1.*",
    "snapshotpl/zf-snap-jquery": "0.5.*",
    "soflomo/log": "dev-master",
    "soflomo/purifier": "1.0.*",
    "swix/swix-em-config": "dev-master",
    "hounddog/doctrine-data-fixture-module": "0.*",
    "firephp/firephp-core": "0.4.*",
    "leafo/scssphp": "^0.6.5",
    "ccampbell/chromephp": "4.*",
