    "simple-bus/asynchronous": "2.*",
    "netglue/zf2-money-module": "0.1.*",
    "prolic/humus-amqp-module": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5.0",
    "codeception/codeception": "2.2.*",
    "phpmd/phpmd": "2.*",
    "sebastian/phpcpd": "2.*",
    "pdepend/pdepend": "2.*",
    "phploc/phploc": "3.*",
    "squizlabs/php_codesniffer": "^2.6.1",
    "satooshi/php-coveralls": "~1.0"
  },
  "suggest": {
    "ext-xdebug": "Extension which provides debugging and profiling capabilities, make possible code coverage support."
  },
  "config": {
    "bin-dir": "bin/",
    "process-timeout": 4000,
    "platform": {
      "ext-mongo": "1.6"
    },
    "preferred-install": "dist",
    "component-dir": "public/assets/lib",
    "component-baseurl": "/assets/lib"
  },
  "scripts": {
    "post-install-cmd": [
      "WellCart\\Setup\\Composer\\ScriptHandler::setPermissions"
    ],
    "post-update-cmd": [
      "WellCart\\Setup\\Composer\\ScriptHandler::setPermissions"
    ],
    "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
    "test": "codecept run",
    "coveralls": "coveralls",
    "cs": "phpcs",
    "cs-fix": "phpcbf"
  },
  "extra": {
    "wellcart": {
      "type": "project",
      "context": {
        "global": {
          "name": "global",
          "extends": false,
          "require": {}
        },
        "common": {
          "name": "common",
          "extends": false,
          "exclude": {
            "70000": "WellCart\\Setup"
          },
          "require": {
            "-90500": "ConLayout",
            "-90400": "ConVarnish",
            "-90000": "WellCart\\Theme\\FrontendUi",
            "-72948": "ZfcBase",
            "-72947": "ZfcTwig",
            "-72946": "HumusAmqpModule",
            "-72945": "ZeTheme",
            "-72943": "AcMailer",
            "-72942": "SwixEmConfig",
            "-72941": "BjyProfiler",
            "-72940": "ZendDeveloperTools",
            "-72939": "WellCart\\CommandBus",
            "-72938": "WellCart\\SchemaMigration",
            "-72937": "RdnFactory",
            "-72936": "RdnRequireJS",
            "-72935": "AssetManager",
            "-72934": "TwbBundle",
            "-72933": "ZfSnapJquery",
            "-72932": "ZfcDatagrid",
            "-72931": "AtPhpSettings",
            "-72930": "StrokerForm",
            "-72929": "HtImgModule",
            "-72928": "RdnCsv",
            "-72927": "Wizard",
            "-72926": "Soflomo\\Log",
            "-72925": "Soflomo\\Purifier",
            "-72924": "DoctrineModule",
            "-72923": "DoctrineMongoODMModule",
            "-72922": "DoctrineORMModule",
            "-72920": "DoctrineDataFixtureModule",
            "-72919": "SlmQueue",
            "-72918": "SlmQueueDoctrine",
            "-72917": "BitWeb\\CronModule",
            "-72900": "WellCart\\Base",
            "-72852": "ZfcUser",
            "-72851": "ZfcUserDoctrineORM",
            "-72850": "ZfcRbac",
            "-72820": "WellCart\\User",
            "-72810": "ZfcAdmin",
            "-72800": "WellCart\\Admin",
            "-72750": "WellCart\\RestApi",
            "20": "WellCart\\CMS",
            "29": "NetglueMoney",
            "30": "WellCart\\Directory",
            "40": "WellCart\\Catalog"
          }
        },
        "console": {
          "name": "console",
          "extends": "common",
          "require": {}
        },
        "api": {
          "name": "api",
          "extends": "common",
          "require": {
            "-72778": "ZF\\Configuration",
            "-72777": "ZF\\Apigility",
            "-72776": "ZF\\Apigility\\Documentation",
            "-72775": "ZF\\Apigility\\Documentation\\Swagger",
            "-72774": "ZF\\Apigility\\Provider",
            "-72773": "ZF\\ApiProblem",
            "-72772": "ZF\\MvcAuth",
            "-72771": "ZF\\OAuth2",
            "-72770": "ZF\\Hal",
            "-72769": "ZF\\ContentNegotiation",
            "-72768": "ZF\\ContentValidation",
            "-72767": "ZF\\Rest",
            "-72765": "ZF\\Versioning",
            "-72764": "ZF\\Doctrine\\QueryBuilder",
            "-72760": "Phpro\\DoctrineHydrationModule",
            "-72759": "ZF\\Apigility\\Doctrine\\Server",
            "-72758": "ZF\\OAuth2\\Doctrine",
            "-72757": "ZF\\OAuth2\\Doctrine\\Console",
            "-72750": "WellCart\\RestApi"
          }
        },
        "frontend": {
          "name": "frontend",
          "extends": "common",
          "require": {
            "-80400": "WellCart\\Theme\\BackendUi"
          }
        },
        "backend": {
          "name": "backend",
          "extends": "frontend",
          "require": {
            "-80400": "WellCart\\Theme\\BackendUi"
          }
        },
        "setup": {
          "name": "setup",
          "extends": false,
          "require": {
            "-90500": "ConLayout",
            "-90400": "ConVarnish",
            "-90000": "WellCart\\Theme\\FrontendUi",
            "-80200": "WellCart\\Theme\\SetupUi",
            "-72931": "ZfcBase",
            "-72929": "ZeTheme",
            "-72926": "SwixEmConfig",
            "-72925": "BjyProfiler",
            "-72922": "RdnFactory",
            "-72921": "RdnRequireJS",
            "-72920": "AssetManager",
            "-72919": "TwbBundle",
            "-72916": "AtPhpSettings",
            "-72915": "StrokerForm",
            "-72913": "WellCart\\CommandBus",
            "-72912": "Wizard",
            "-72911": "Soflomo\\Log",
            "-72750": "WellCart\\RestApi",
            "-72900": "WellCart\\Base",
            "-72938": "ZendDeveloperTools",
            "70000": "WellCart\\Setup"
          }
        }
      }
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev",
      "dev-develop": "0.2.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "WellCart\\Base\\": "component/base/src/",
      "WellCart\\Admin\\": "component/admin/src/",
      "WellCart\\Setup\\": "component/setup/src/",
      "WellCart\\User\\": "component/user/src/",
      "WellCart\\RestApi\\": "component/rest-api/src/",
      "WellCart\\Catalog\\": "module/catalog/src/",
      "WellCart\\CMS\\": "module/cms/src/",
      "WellCart\\Directory\\": "module/directory/src/"
    },
    "files": [
      "component/base/src/functions.php",
      "component/utility/src/functions.php"
    ],
    "classmap": [
      "component/library/",
      "component/mapping-drivers/",
      "component/schema-migration/src/",
      "component/command-bus/src/",
      "component/utility/src/",
      "theme/frontend-ui/",
      "theme/backend-ui/",
      "theme/setup-ui/"
    ]
  }
}
