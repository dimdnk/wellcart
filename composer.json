{
  "name": "wellcart/wellcart",
  "description": "WellCart Application",
  "license": "BSD-3-Clause",
  "homepage": "http://wellcart.org/",
  "type": "project",
  "authors": [
    {
      "name": "Dima Denisenko",
      "email": "dima@wellcart.org",
      "role": "Developer"
    },
    {
      "name": "WellCart Application Contributors",
      "homepage": "https://github.com/wellcart/wellcart/contributors"
    }
  ],
  "keywords": [
    "Zend Framework",
    "Open-source"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1.0",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-openssl": "*",
    "lib-pcre": ">=7.3",
    "ext-iconv": "*",
    "ext-mbstring": "*",
    "ext-xml": "*",
    "ext-libxml": "*",
    "ext-intl": "*",
    "jbroadway/urlify": "1.*",
    "patchwork/utf8": "1.*",
    "raulfraile/ladybug": "1.*",
    "nesbot/carbon": "1.*",
    "ext-PDO": "*",
    "ext-pdo_mysql": "*",
    "lib-libxml": "*",
    "ext-ctype": "*",
    "ext-hash": "*",
    "ext-spl": "*",
    "ext-gd": "*",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "ext-xsl": "*",
    "antasoft/web-components": "^0.1.0",
    "josegonzalez/dotenv": "~3.0",
    "zendframework/zend-router": "2.7.*", 
    "zendframework/zend-servicemanager": "2.7.*",
    "zendframework/zend-router": "2.7.*",
    "zendframework/zend-developer-tools": "1.1.*",
    "container-interop/container-interop": "^1.2.0",
    "adlogix/zf2-opensoft-rollout": "^1.0.0",
    "gedmo/doctrine-extensions": "2.4.*",
    "doctrine/doctrine-module": "^1.2.0",
    "doctrine/doctrine-mongo-odm-module": "0.11.*",
    "beberlei/DoctrineExtensions": "1.*",
    "slm/queue": "1.0.*",
    "slm/queue-doctrine": "1.0.*",
    "bitweb/zf2-cron-module": "2.0.*",
    "zendexperts/ze-theme": "1.0.*@dev",
    "hummer2k/conlayout": "^3.2",
    "hummer2k/convarnish": "^2.0",
    "bjyoungblood/bjy-profiler": "1.*",
    "rwoverdijk/assetmanager": "~1.7.0",
    "tck/zf2-imageresizer": "1.*",
    "radnan/rdn-factory": "1.*",
    "radnan/rdn-require-js": "1.*",
    "acelaya/zf2-acmailer": "5.*",
    "neilime/zf2-twb-bundle": "3.*@stable",
    "thadafinser/zfc-datagrid": "1.3.*",
    "neeckeloo/wizard": "1.0.1",
    "stroker/form": "^1.0.0",
    "snapshotpl/zf-snap-jquery": "0.5.*",
    "soflomo/purifier": "1.0.*",
    "swix/swix-em-config": "dev-master",
    "hounddog/doctrine-data-fixture-module": "0.*",
    "firephp/firephp-core": "0.4.*",
    "leafo/scssphp": "^0.6.5",
    "ccampbell/chromephp": "4.*",
    "oro/doctrine-extensions": "^1.0.0",
    "zf-commons/zfc-user": "~1.4.0",
    "zf-commons/zfc-user-doctrine-orm": "1.*",
    "zf-commons/zfc-rbac": "^2.6.0",
    "zf-commons/zfc-admin": "0.2.*",
    "robmorgan/phinx": "0.8.*",
    "fzaninotto/faker": "1.6.*",
    "simple-bus/asynchronous": "2.*",
    "netglue/zf2-money-module": "0.1.*",
    "prolic/humus-amqp-module": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit-skeleton-generator": "*", 
    "phpunit/phpunit": "^5.7.0",
    
    "edgedesign/phpqa": "^1.9.1",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "jakub-onderka/php-console-highlighter": "^0.3.2",
    "phpstan/phpstan": "^0.6.3",
    "satooshi/php-coveralls": "~1.0"
  },
  "suggest": {
    "ext-xdebug": "Extension which provides debugging and profiling capabilities, make possible code coverage support."
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "process-timeout": 4000,
    "platform": {
      "ext-mongo": "1.6"
    },
    "preferred-install": "dist",
    "component-dir": "public/assets/lib",
    "component-baseurl": "/assets/lib"
  },
  "scripts": {
    "post-install-cmd": [
      "WellCart\\Setup\\Composer\\ScriptHandler::createProjectDirectories",
      "WellCart\\Setup\\Composer\\ScriptHandler::setPermissions"
    ],
    "post-update-cmd": [
      "WellCart\\Setup\\Composer\\ScriptHandler::createProjectDirectories",
      "WellCart\\Setup\\Composer\\ScriptHandler::setPermissions"
    ],
    "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
    "coveralls": "coveralls",
    "cs": "phpcs",
    "cs-fix": "phpcbf"
  },
  "extra": {
    "wellcart": {
      "type": "project",
      "context": {
        "global": {
          "name": "global",
          "extends": false,
          "require": {}
        },
        "common": {
          "name": "common",
          "extends": false,
          "exclude": {
            "70000": "WellCart\\Setup"
          },
          "require": {
            "-90500": "ConLayout",
            "-90400": "ConVarnish",
            "-90000": "WellCart\\Theme\\FrontendUi",
            "-72949": "Adlogix\\Zf2Rollout",
            "-72948": "ZfcBase",
            "-72946": "HumusAmqpModule",
            "-72945": "ZeTheme",
            "-72943": "AcMailer",
            "-72942": "SwixEmConfig",
            "-72941": "BjyProfiler",
            "-72940": "ZendDeveloperTools",
            "-72939": "WellCart\\CommandBus",
            "-72938": "WellCart\\SchemaMigration",
            "-72937": "RdnFactory",
            "-72936": "RdnRequireJS",
            "-72935": "AssetManager",
            "-72934": "TwbBundle",
            "-72933": "ZfSnapJquery",
            "-72932": "ZfcDatagrid",
            "-72930": "StrokerForm",
            "-72929": "TckImageResizer",
            "-72927": "Wizard",
            "-72925": "Soflomo\\Purifier",
            "-72924": "DoctrineModule",
            "-72923": "DoctrineMongoODMModule",
            "-72922": "DoctrineORMModule",
            "-72920": "DoctrineDataFixtureModule",
            "-72919": "SlmQueue",
            "-72918": "SlmQueueDoctrine",
            "-72917": "BitWeb\\CronModule",
            "-72900": "WellCart\\Base",
            "-72852": "ZfcUser",
            "-72851": "ZfcUserDoctrineORM",
            "-72850": "ZfcRbac",
            "-72820": "WellCart\\User",
            "-72810": "ZfcAdmin",
            "-72800": "WellCart\\Backend",
            "20": "WellCart\\CMS",
            "29": "NetglueMoney",
            "30": "WellCart\\Directory",
            "40": "WellCart\\Catalog"
          }
        },
        "console": {
          "name": "console",
          "extends": "common",
          "require": {}
        },
        "frontend": {
          "name": "frontend",
          "extends": "common",
          "require": {
            "-80400": "WellCart\\Theme\\BackendUi"
          }
        },
        "backend": {
          "name": "backend",
          "extends": "frontend",
          "require": {
            "-80400": "WellCart\\Theme\\BackendUi"
          }
        },
        "setup": {
          "name": "setup",
          "extends": false,
          "require": {
            "-90500": "ConLayout",
            "-90400": "ConVarnish",
            "-90000": "WellCart\\Theme\\FrontendUi",
            "-80200": "WellCart\\Theme\\SetupUi",
            "-72931": "ZfcBase",
            "-72929": "ZeTheme",
            "-72926": "SwixEmConfig",
            "-72925": "BjyProfiler",
            "-72922": "RdnFactory",
            "-72921": "RdnRequireJS",
            "-72920": "AssetManager",
            "-72919": "TwbBundle",
            "-72915": "StrokerForm",
            "-72913": "WellCart\\CommandBus",
            "-72912": "Wizard",
            "-72900": "WellCart\\Base",
            "-72938": "ZendDeveloperTools",
            "70000": "WellCart\\Setup"
          }
        }
      }
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev",
      "dev-develop": "0.2.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "WellCart\\Base\\": "component/base/src/",
      "WellCart\\Backend\\": "component/backend/src/",
      "WellCart\\Setup\\": "component/setup/src/",
      "WellCart\\User\\": "component/user/src/",
      "WellCart\\Catalog\\": "module/catalog/src/",
      "WellCart\\CMS\\": "module/cms/src/",
      "WellCart\\Directory\\": "module/directory/src/"
    },
    "files": [
      "component/base/src/functions.php",
      "component/library/utility/src/functions.php"
    ],
    "classmap": [
      "component/library/",
      "component/library/mapping-drivers/",
      "component/schema-migration/src/",
      "component/command-bus/src/",
      "component/library/utility/src/",
      "theme/frontend-ui/",
      "theme/backend-ui/",
      "theme/setup-ui/"
    ]
  }
}
