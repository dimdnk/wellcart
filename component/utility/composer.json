{
  "name": "wellcart/component-utility",
  "description": "WellCart Utility Library",
  "license": "BSD-3-Clause",
  "homepage": "http://wellcart.org/",
  "type": "library",
  "authors": [
    {
      "name": "Dima Denisenko",
      "email": "dima@wellcart.org",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.0.0",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-openssl": "*",
    "lib-pcre": ">=7.3",
    "ext-iconv": "*",
    "ext-mbstring": "*",
    "ext-xml": "*",
    "ext-libxml": "*",
    "ext-intl": "*",
    "jbroadway/urlify": "1.*",
    "patchwork/utf8": "1.*",
    "raulfraile/ladybug": "1.*",
    "nesbot/carbon": "1.*",
    "josegonzalez/dotenv": "~2.0"
  },
  "suggest": {
    "halilim/xml-iterator": "dev-master",
    "gabrielelana/byte-units": "0.1.*",
    "league/fractal": "0.*",
    "triplepoint/php-units-of-measure": "2.*",
    "monolog/monolog": "1.*",
    "d11wtq/boris": "1.0.*",
    "imagine/imagine": "0.6.*",
    "mtdowling/cron-expression": "1.*",
    "bnet/bnmoney": "1.*@dev",
    "league/csv": "7.*",
    "jwage/purl": "dev-master",
    "league/flysystem": "1.*",
    "jms/serializer": "0.16.*",
    "herrera-io/file-locator": "1.0.*@dev",
    "giggsey/libphonenumber-for-php": "~7.0",
    "justinrainbow/json-schema": "~1.3",
    "jbzoo/utils": "1.4.*",
    "ext-posix": "*",
    "ext-dom": "*",
    "ext-PDO": "*",
    "ext-pdo_mysql": "*",
    "ext-mysql": "*",
    "ext-gd": "*",
    "ext-http": "*",
    "ext-readline": "*",
    "ext-pcntl": "*",
    "ext-xdebug": "Extension which provides debugging and profiling capabilities, make possible code coverage support."
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5.0",
    "phpmd/phpmd": "2.*",
    "sebastian/phpcpd": "2.*",
    "pdepend/pdepend": "2.*",
    "phploc/phploc": "3.*",
    "squizlabs/php_codesniffer": "^2.6.1",
    "satooshi/php-coveralls": "~1.0"
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "process-timeout": 4000,
    "preferred-install": "dist",
    "platform": {
      "ext-mongo": "1.6"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "WellCart\\Utility\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  }
}
