{
  "name": "wellcart/component-view",
  "description": "The components of the view layer.",
  "license": "BSD-3-Clause",
  "homepage": "http://wellcart.org/",
  "type": "library",
  "authors": [
    {
      "name": "Dima Denisenko",
      "email": "dima@wellcart.org",
      "role": "Developer"
    }
  ],
  "config": {
    "bin-dir": "bin/",
    "process-timeout": 4000,
    "sort-packages": true,
    "preferred-install": "dist",
    "platform": {
      "ext-mongo": "1.6"
    }
  },
  "extra": {
    "wellcart": {
      "type": "component",
      "name": "WellCart\\View"
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.0.0",
    "zendframework/zend-view": "2.8.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.0",
    "codeception/codeception": "2.2.*",
    "edgedesign/phpqa": "^1.9.1",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "jakub-onderka/php-console-highlighter": "^0.3.2",
    "phpstan/phpstan": "^0.6.3",
    "satooshi/php-coveralls": "~1.0"
  },
  "suggest": {
    "ext-xdebug": "Extension which provides debugging and profiling capabilities, make possible code coverage support."
  },
  "autoload": {
    "psr-4": {
      "WellCart\\View\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
    ]
  }
}