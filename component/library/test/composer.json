{
  "name": "wellcart/component-test",
  "description": "Test component from WellCart.",
  "license": "BSD-3-Clause",
  "homepage": "http://wellcart.org/",
  "type": "library",
  "authors": [
    {
      "name": "Dima Denisenko",
      "email": "dima@wellcart.org",
      "role": "Developer"
    }
  ],
  "config": {
    "bin-dir": "bin/",
    "process-timeout": 4000,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "extra": {
    "wellcart": {
      "type": "component",
      "name": "WellCart\\Test"
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1.3",
    "zendframework/zend-test": "2.5.*",
    "phpunit/phpunit": "^5.7.23",
    "codedungeon/phpunit-result-printer": "^0.4.4"
  },
  "require-dev": {
    "edgedesign/phpqa": "^1.15",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "jakub-onderka/php-console-highlighter": "^0.3.2",
    "phpbench/phpbench": "^0.13",
    "phpstan/phpstan": "^0.7",
    "satooshi/php-coveralls": "~1.0"
  },
  "suggest": {
    "ext-xdebug": "Extension which provides debugging and profiling capabilities, make possible code coverage support."
  },
  "autoload": {
    "classmap": [
      "./lib"
    ],
    "psr-4": {
      "WellCart\\Test\\": "src/"
    }
  },
  "autoload-dev": {
  }
}