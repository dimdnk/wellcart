{
  "name": "wellcart/component-ui-wizard",
  "description": "Wizard module for Zend Framework",
  "license": "BSD-3-Clause",
  "homepage": "http://wellcart.org/",
  "type": "library",
  "authors": [
    {
      "name": "Dima Denisenko",
      "email": "dima@wellcart.org",
      "role": "Developer"
    }
  ],
  "config": {
    "bin-dir": "bin/",
    "process-timeout": 4000,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "extra": {
    "wellcart": {
      "type": "component",
      "name": "WellCart\\Ui\\Wizard"
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1.0",
    "wellcart/component-ui": "^0.1.0",
    "symfony/options-resolver": "~2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.23",
    "edgedesign/phpqa": "^1.15",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "jakub-onderka/php-console-highlighter": "^0.3.2",
    "phpstan/phpstan": "^0.7",
    "satooshi/php-coveralls": "~1.0"
  },
  "suggest": {
    "ext-xdebug": "Extension which provides debugging and profiling capabilities, make possible code coverage support."
  },
  "autoload": {
    "psr-4": {
      "WellCart\\Ui\\Wizard\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
    ]
  }
}