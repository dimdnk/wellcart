{
  "name": "wellcart/component-ui",
  "description": "Provides a set of commonly needed UI elements",
  "license": "BSD-3-Clause",
  "homepage": "http://wellcart.org/",
  "type": "library",
  "version": "0.1.0",
  "authors": [
    {
      "name": "Dima Denisenko",
      "email": "dima@wellcart.org",
      "role": "Developer"
    }
  ],
  "config": {
    "bin-dir": "bin/",
    "process-timeout": 4000,
    "preferred-install": "dist",
    "platform": {
      "ext-mongo": "1.6"
    }
  },
  "extra": {
    "wellcart": {
      "type": "component",
      "name": "WellCart\\Ui"
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.0.0",
    "rwoverdijk/assetmanager": "~1.6.0",
    "zendexperts/ze-theme": "1.0.*@dev",
    "hummer2k/conlayout": "^3.2",
    "hummer2k/convarnish": "^2.0",
    "neilime/zf2-twb-bundle": "2.*@stable",
    "neeckeloo/wizard": "1.0.1",
    "stroker/form": "0.2.0",
    "thadafinser/zfc-datagrid": "1.2.*",
    "snapshotpl/zf-snap-jquery": "0.5.*",
    "robloach/component-installer": "0.2.3",
    "components/jquery": "~3.1.0",
    "components/jqueryui": "1.12.*",
    "components/jquery-cookie": "1.4.*",
    "components/ember": "2.8.*",
    "components/ember-data": "2.8.*",
    "eonasdan/bootstrap-datetimepicker": "4.17.*",
    "moment/moment": "2.*",
    "oro/moment-timezone": "0.3.*",
    "drmonty/chosen": "1.6.*",
    "ivaynberg/select2": "4.0.*",
    "components/bootstrap": "3.3.*",
    "components/font-awesome": "4.6.*",
    "components/handlebars.js": "4.0.*",
    "blueimp/jquery-file-upload": "9.12.*",
    "gdarko/twitter-bootstrap-grid": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5.0",
    "phpmd/phpmd": "2.*",
    "sebastian/phpcpd": "2.*",
    "pdepend/pdepend": "2.*",
    "phploc/phploc": "3.*",
    "squizlabs/php_codesniffer": "^2.6.1",
    "satooshi/php-coveralls": "~1.0"
  },
  "suggest": {
    "ext-xdebug": "Extension which provides debugging and profiling capabilities, make possible code coverage support."
  },
  "autoload": {
    "psr-4": {
      "WellCart\\Ui\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
    ]
  }
}