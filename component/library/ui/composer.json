{
  "name": "wellcart/component-ui",
  "description": "Provides a set of commonly needed UI elements",
  "license": "BSD-3-Clause",
  "homepage": "http://wellcart.org/",
  "type": "library",
  "authors": [
    {
      "name": "Dima Denisenko",
      "email": "dima@wellcart.org",
      "role": "Developer"
    }
  ],
  "config": {
    "bin-dir": "bin/",
    "process-timeout": 4000,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "extra": {
    "wellcart": {
      "type": "component",
      "name": "WellCart\\Ui"
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.2.0",
    "rwoverdijk/assetmanager": "~1.7.0",
    "neilime/zf2-twb-bundle": "3.*@stable",
    "zfc-datagrid/zfc-datagrid": "1.3.*",
    "antasoft/web-components": "^0.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7.23",
    "codedungeon/phpunit-result-printer": "^0.4.4",
    "edgedesign/phpqa": "^1.15",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "jakub-onderka/php-console-highlighter": "^0.3.2",
    "phpbench/phpbench": "^0.13",
    "phpstan/phpstan": "^0.7",
    "satooshi/php-coveralls": "~1.0"
  },
  "suggest": {
    "ext-xdebug": "Extension which provides debugging and profiling capabilities, make possible code coverage support."
  },
  "autoload": {
    "psr-4": {
      "WellCart\\Ui\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
    ]
  }
}