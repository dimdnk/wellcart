{
  "name": "wellcart/component-admin",
  "description": "Common infrastructure and assets for other modules to be defined and used in their administration user interface.",
  "license": "BSD-3-Clause",
  "homepage": "http://wellcart.org/",
  "type": "library",
  "version": "0.1.0",
  "authors": [
    {
      "name": "Dima Denisenko",
      "email": "dima@wellcart.org",
      "role": "Developer"
    },
    {
      "name": "WellCart Admin Contributors",
      "homepage": "https://github.com/wellcart/component-admin/contributors"
    }
  ],
  "config": {
    "bin-dir": "bin/",
    "process-timeout": 4000,
    "platform": {
      "ext-mongo": "1.6"
    },
    "preferred-install": "dist",
    "component-dir": "tests/public/assets/lib",
    "component-baseurl": "/assets/lib"
  },
  "scripts": {
    "post-autoload-dump": [
      "php -r \"chmod('tests/config/autoload', 0777);\"",
      "php -r \"chmod('tests/public/assets', 0777);\"",
      "php -r \"chmod('tests/public/themes', 0777);\"",
      "php -r \"chmod('tests/public/media', 0777);\"",
      "php -r \"chmod('tests/data', 0777);\"",
      "php -r \"chmod('tests/data/cache', 0777);\"",
      "php -r \"chmod('tests/data/logs', 0777);\"",
      "php -r \"chmod('tests/data/db/migrations', 0777);\"",
      "php -r \"chmod('tests/data/db/seeds', 0777);\"",
      "php -r \"chmod('tests/data/sessions', 0777);\"",
      "php -r \"chmod('tests/data/upload', 0777);\""
    ],
    "test": "codecept run",
    "coveralls": "coveralls",
    "cs": "phpcs",
    "cs-fix": "phpcbf"
  },
  "extra": {
    "wellcart": {
      "type": "component",
      "name": "WellCart\\Admin",
      "context": {
        "common": {
          "name": "common",
          "extends": false,
          "require": {
            "-72810": "ZfcAdmin",
            "-72800": "WellCart\\Admin"
          }
        }
      }
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.0.0",
    "zf-commons/zfc-admin": "0.2.*",
    "wellcart/component-base": "0.1.*",
    "wellcart/component-setup": "0.1.*",
    "wellcart/component-rest-api": "0.1.*",
    "wellcart/component-user": "0.1.*",
    "wellcart/theme-backend-ui": "0.1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5.0",
    "codeception/codeception": "2.2.*",
    "phpmd/phpmd": "2.*",
    "sebastian/phpcpd": "2.*",
    "pdepend/pdepend": "2.*",
    "phploc/phploc": "3.*",
    "squizlabs/php_codesniffer": "^2.6.1",
    "satooshi/php-coveralls": "~1.0"
  },
  "suggest": {
    "ext-xdebug": "Extension which provides debugging and profiling capabilities, make possible code coverage support."
  },
  "autoload": {
    "psr-4": {
      "WellCart\\Admin\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/unit/"
    ]
  }
}