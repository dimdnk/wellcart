{
  "name": "wellcart/component-base",
  "description": "This is a suite of common classes used across several modules. Extends some MVC, Form, View, Validator & Doctrine ORM functionality.",
  "license": "BSD-3-Clause",
  "homepage": "http://wellcart.org/",
  "type": "library",
  "version": "0.1.0",
  "authors": [
    {
      "name": "Dima Denisenko",
      "email": "dima@wellcart.org",
      "role": "Developer"
    },
    {
      "name": "WellCart Base Contributors",
      "homepage": "https://github.com/wellcart/component-base/contributors"
    }
  ],
  "config": {
    "bin-dir": "bin/",
    "process-timeout": 4000,
    "platform": {
      "ext-mongo": "1.6"
    },
    "preferred-install": "dist",
    "component-dir": "tests/public/assets/lib",
    "component-baseurl": "/assets/lib"
  },
  "scripts": {
    "post-install-cmd": [
      "WellCart\\Setup\\Composer\\ScriptHandler::setPermissions"
    ],
    "post-update-cmd": [
      "WellCart\\Setup\\Composer\\ScriptHandler::setPermissions"
    ],
    "test": "codecept run",
    "coveralls": "coveralls",
    "cs": "phpcs",
    "cs-fix": "phpcbf"
  },
  "extra": {
    "wellcart": {
      "type": "component",
      "name": "WellCart\\Base",
      "context": {
        "common": {
          "name": "common",
          "extends": false,
          "require": {
            "-90500": "ConLayout",
            "-90400": "ConVarnish",
            "-90000": "WellCart\\Theme\\FrontendUi",
            "-72948": "ZfcBase",
            "-72947": "ZfcTwig",
            "-72946": "HumusAmqpModule",
            "-72945": "ZeTheme",
            "-72943": "AcMailer",
            "-72942": "SwixEmConfig",
            "-72941": "BjyProfiler",
            "-72940": "ZendDeveloperTools",
            "-72939": "WellCart\\CommandBus",
            "-72938": "WellCart\\SchemaMigration",
            "-72937": "RdnFactory",
            "-72936": "RdnRequireJS",
            "-72935": "AssetManager",
            "-72934": "TwbBundle",
            "-72933": "ZfSnapJquery",
            "-72932": "ZfcDatagrid",
            "-72931": "AtPhpSettings",
            "-72930": "StrokerForm",
            "-72929": "HtImgModule",
            "-72928": "RdnCsv",
            "-72927": "Wizard",
            "-72926": "Soflomo\\Log",
            "-72925": "Soflomo\\Purifier",
            "-72924": "DoctrineModule",
            "-72923": "DoctrineMongoODMModule",
            "-72922": "DoctrineORMModule",
            "-72921": "DoctrineViewer",
            "-72920": "DoctrineDataFixtureModule",
            "-72919": "SlmQueue",
            "-72918": "SlmQueueDoctrine",
            "-72917": "BitWeb\\CronModule",
            "-72900": "WellCart\\Base"
          }
        },
        "console": {
          "require": {
          }
        },
        "setup": {
          "name": "setup",
          "extends": false,
          "require": {
            "-90500": "ConLayout",
            "-90400": "ConVarnish",
            "-72931": "ZfcBase",
            "-72929": "ZeTheme",
            "-72926": "SwixEmConfig",
            "-72925": "BjyProfiler",
            "-72922": "RdnFactory",
            "-72921": "RdnRequireJS",
            "-72920": "AssetManager",
            "-72919": "TwbBundle",
            "-72916": "AtPhpSettings",
            "-72915": "StrokerForm",
            "-72913": "WellCart\\CommandBus",
            "-72912": "Wizard",
            "-72911": "Soflomo\\Log",
            "-72900": "WellCart\\Base"
          }
        }
      }
    },
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.0.0",
    "wellcart/component-library": "0.1.*",
    "wellcart/component-setup": "0.1.*",
    "wellcart/component-admin": "0.1.*",
    "wellcart/component-rest-api": "0.1.*",
    "wellcart/component-user": "0.1.*",
    "wellcart/theme-frontend-ui": "0.1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5.0",
    "codeception/codeception": "2.2.*",
    "phpmd/phpmd": "2.*",
    "sebastian/phpcpd": "2.*",
    "pdepend/pdepend": "2.*",
    "phploc/phploc": "3.*",
    "squizlabs/php_codesniffer": "^2.6.1",
    "satooshi/php-coveralls": "~1.0"
  },
  "suggest": {
    "ext-xdebug": "Extension which provides debugging and profiling capabilities, make possible code coverage support."
  },
  "autoload": {
    "psr-4": {
      "WellCart\\Base\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/unit/"
    ]
  },
  "bin": [
    "bin/wellcart"
  ]
}
